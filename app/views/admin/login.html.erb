<section class="login">
  <h1>Login</h1>
  <%= form_tag do %>
    <div> <%= text_field_tag(:username, "", placeholder: "Username")%> </div>
    <div> <%= password_field_tag(:password, "", placeholder: "Password") %> </div>
    <div> <%= submit_tag "Log in", class: 'submit-button' %> </div>
  <% end %>
</section>
